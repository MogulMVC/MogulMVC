cp ~/Workspace/Aptana/Mogul\ Frameworks/Mogul\ Icons/img/* ~/Workspace/Aptana/Mogul\ Frameworks/Mogul\ MVC/Framework/framework/img/icon/
#rm ~/Workspace/Aptana/Mogul\ Frameworks/Mogul\ MVC/Framework/framework/lib/css/core/MIcons*
cp ~/Workspace/Aptana/Mogul\ Frameworks/Mogul\ Icons/bin/* ~/Workspace/Aptana/Mogul\ Frameworks/Mogul\ MVC/Framework/framework/lib/css/core/

#JS
