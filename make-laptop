rm ~/Workspace/Aptana/Mogul\ Frameworks/Mogul\ MVC/Framework/framework/lib/js/core/M*
cp ~/Workspace/Aptana/Mogul\ Frameworks/Mogul\ JS/bin/* ~/Workspace/Aptana/Mogul\ Frameworks/Mogul\ MVC/Framework/framework/lib/js/core/