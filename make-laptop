#!/bin/bash

#CSS
cd ~/Workspace/Aptana/Mogul\ Frameworks/Mogul\ CSS/
./make
rm ~/Workspace/Aptana/Mogul\ Frameworks/Mogul\ MVC/Framework/framework/lib/css/core/M*
